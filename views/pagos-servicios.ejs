    <div class="container-fluid mt-2">
        <h2 class="text-secondary">Pagos Servicios</h2>
        <hr>
        <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#exampleModal"
            data-bs-whatever="@fat">Cargar Pago</button>

        <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">Pago Servicio</h5>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">

                        <form method="POST">
                            <div class=" ">



                                <select name="descripcionServicio" id="" class="form-control">
                                    <option value="" class="form-control">Servicio</option>
                                    <option value="Sistema Sispres">Sistema Sispres</option>
                                    <option value="Servicio de Hosting Web">Servicio de Hosting Web</option>
                                    <option value="Servicios Varios">Servicios Varios</option>
                                </select>

                                <select name="tipoPagoServicio" id="" class="form-control">
                                    <option value="">Tipo de Pago</option>
                                    <option value="Semanal">Semanal</option>
                                    <option value="Quincenal">Quincenal</option>
                                    <option value="Mensual">Mensual</option>
                                </select>

                                <select name="mesPagoServicio" id="" class="form-control">
                                    <option value="">Mes Pago</option>
                                    <option class="text-dark" value="Enero">Enero</option>
                                    <option class="text-dark" value="Febrero">Febrero</option>
                                    <option class="text-dark" value="Marzo">Marzo</option>
                                    <option class="text-dark" value="Abril">Abril</option>
                                    <option class="text-dark" value="Mayo">Mayo</option>
                                    <option class="text-dark" value="Junio">Junio</option>
                                    <option class="text-dark" value="Julio">Julio</option>
                                    <option class="text-dark" value="Agosto">Agosto</option>
                                    <option class="text-dark" value="Septiembre">Septiembre</option>
                                    <option class="text-dark" value="Obtubre">Obtubre</option>
                                    <option class="text-dark" value="Noviembre">Noviembre</option>
                                    <option class="text-dark" value="Diciembre">Diciembre</option>
                                </select>
                                <input type="number" name="montoPagoServicio" placeholder="Monto" class="form-control">
                                <input type="date" name="fechaPagoServicio" placeholder="Fecha" class="form-control">
                                <input type="text" name="observacionPagoServicio" placeholder="Observacion"
                                    class="form-control ">

                                <button type="submit" class="btn btn-success">Cargar Pago</button>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                                <button type="button" class="btn btn-primary">Send message</button>
                            </div>
                        </form>
                    </div>

                </div>
            </div>
        </div>
    </div>



    <div class="container-fluid d-xxl-flex justify-content-around"
